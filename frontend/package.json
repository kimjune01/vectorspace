{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:social": "playwright test --project=chromium tests/e2e/social-features.spec.ts",
    "test:e2e:profiles": "playwright test --project=chromium tests/e2e/user-profiles.spec.ts",
    "test:e2e:search": "playwright test --project=chromium tests/e2e/semantic-search.spec.ts",
    "test:e2e:auth": "playwright test --project=chromium tests/e2e/auth-enhanced.spec.ts",
    "test:e2e:presence": "playwright test --project=chromium-multi-user tests/e2e/presence-system.spec.ts",
    "test:e2e:connections": "playwright test --project=chromium-multi-user tests/e2e/human-connections.spec.ts",
    "test:e2e:multi-user": "playwright test --project=chromium-multi-user tests/e2e/multi-user-scenarios.spec.ts",
    "test:e2e:performance": "playwright test --project=chromium-performance tests/e2e/performance-benchmarks.spec.ts",
    "test:e2e:integration": "playwright test --project=chromium-performance tests/e2e/integration-workflows.spec.ts",
    "test:e2e:all-browsers": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome'",
    "test:e2e:headed": "playwright test --headed --project=chromium",
    "test:e2e:debug": "playwright test --debug --project=chromium"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
